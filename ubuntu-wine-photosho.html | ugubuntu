<script type="text/javascript">
today=new Date();
var day; var date; var hello;
hour=new Date().getHours()
if(hour < 4)hello='تاڭ يۇرۇش ئالدىدا !'
else if(hour < 7)hello=' سالام سەھەر ! '
else if(hour < 10)hello=' چۈشتىن بۇرۇن '
else if(hour < 12)hello='چۈش ۋاقتى '
else if(hour < 15)hello=' چۈشتىن كىيىن '
else if(hour < 20)hello=' كەچلىكىڭىز خەيرىلىك بولسۇن ! '
else {hello='تۈن يېرىم '}
var webUrl = webUrl;
document.write(' '+hello);
</script>
<span id=localtime>
<script type="text/javascript">
function showLocale(objD)
{
var str,colorhead,colorfoot;
var yy = objD.getYear();
if(yy<1900) yy = yy+1900;
var MM = objD.getMonth()+1;
if(MM<10) MM = '0' + MM;
var dd = objD.getDate();
if(dd<10) dd = '0' + dd;
var hh = objD.getHours();
if(hh<10) hh = '0' + hh;
var mm = objD.getMinutes();
if(mm<10) mm = '0' + mm;
var ss = objD.getSeconds();
if(ss<10) ss = '0' + ss;
var ww = objD.getDay();
if ( ww==0 ) colorhead="<font color=\"#000\">";
if ( ww > 0 && ww < 6 ) colorhead="<font color=\"#000\">";
if ( ww==6 ) colorhead="<font color=\"#000\">";
if (ww==1) ww="دۈشەنبە";
if (ww==0) ww="يەكشەنبە";
if (ww==2) ww="سەيشەنبە";
if (ww==3) ww="چارشەنبە";
if (ww==4) ww="پەيشەنبە";
if (ww==5) ww="خەيرىلىك جۈمە";
if (ww==6) ww="شەنبە";
colorfoot="</font>"
str = colorhead + yy + "-يىل" + MM + "-ئاينىڭ" + dd + "-كۈنى " + hh + ":" + mm + ":" + ss + " " + ww + colorfoot;
return(str);
}
function tick()
{
var today;
today = new Date();
document.getElementById("localtime").innerHTML = showLocale(today);
window.setTimeout("tick()", 1000);
}
tick();
</script>
</span> 
