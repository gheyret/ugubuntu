today=new Date();
var day; var date; var hello;
hour=new Date().getHours()
if(hour < 6)hello='  تاڭ يۇرۇش ئالدىدا! '
else if(hour < 9)hello=' خەيرىلىك سەھەر! '
else if(hour < 12)hello=' چۈشتىن بۇرۇن '
else if(hour < 14)hello=' خەيىرلىك چۈش! '
else if(hour < 17)hello=' چۈشتىن كىيىن '
else if(hour < 19)hello=' كەچلىكىڭىز خەيرىلىك بولسۇن! '
else if(hour < 22)hello=' خەيىرلىك كەچ! '
else {hello='تۈن يېرىم! '}
var webUrl = webUrl;
document.write(' '+hello);
