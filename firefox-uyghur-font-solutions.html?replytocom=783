	<div class="rssblock">
<script type="text/javascript">	
	var s;
	function showMessage(){
	window.clearTimeout(s);
	document.getElementById("message").style.display="block";
	}
	function hiddenMessage(){
	s=window.setTimeout("hidden();",500);
	}
	function hidden(){
	document.getElementById("message").style.display="none";
	}
</script>

	<h3>بېكەتكە مۇشتەرى بولۇڭ</h3>
	<span onmouseover="showMessage()" onmouseout="hiddenMessage()"><div id="rssfeed"><a href="http://feed.feedsky.com/ugubuntu" title="بېكەتكە مۇشتەرى بولۇڭ" class="image"><img src="http://www.ugubuntu.com/wp-content/themes/HotNewspro/images/rsssub.png" /></a></div></span>
